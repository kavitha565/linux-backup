    constructor(){}
    move(){
        console.log("animal moving!!");
    }
}
class rabbit extends Animal{
     move(){
        console.log("rabbit moving!!");
    }

}
class snake extends Animal{
     move(){
        console.log("rabbit moving!!");
    }
}
